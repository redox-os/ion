echo 1{A{1,2},B{1,2}}
# inclusive ranges
echo {-1...1}
echo {2...0}
echo {a...c}
echo {d...b}
echo {A...C}
echo {D...B}
# exclusive ranges
echo {-1..2}
echo {2..-1}
echo {a..d}
echo {d..a}
echo {A..D}
echo {D..A}
# inclusive stepped ranges
echo {0..2...4}
echo {a..2...e}
echo {A..2...E}
echo {0..-2...-4}
echo {e..-2...a}
echo {E..-2...A}
# exclusive stepped ranges
echo {0..2..5}
echo {a..2..f}
echo {A..2..F}
echo {0..-2..-5}
echo {e..-2..a}
echo {E..-2..A}